# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug_mac
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_mac)
  gta4_config = debug_mac

else ifeq ($(config),release_mac)
  gta4_config = release_mac

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := gta4

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

gta4:
ifneq (,$(gta4_config))
	@echo "==== Building gta4 ($(gta4_config)) ===="
	@${MAKE} --no-print-directory -C . -f gta4.make config=$(gta4_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f gta4.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_mac"
	@echo "  release_mac"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   gta4"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"